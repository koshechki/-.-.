#include <iostream>
int main()
{
    int t;
    std::cin >> t;
    for (int i(0); i < t; i++) {
        int kolvo_1(0);
        for (int j(0); j < 4; j++) {
            int tek;
            std::cin >> tek;
            if (tek == 1) {
                kolvo_1 += 1;
            }
        }
        if (kolvo_1 == 0) {
            std::cout << 0 << "\n";
        }
        else {
            if (kolvo_1 == 4) {
                std::cout << 2 << "\n";
            }
            else {
                std::cout << 1 << "\n";
            }
        }
    }
}
